---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Tiago Bastos</title>
 <link href="http://feeds.feedburner.com/Bastos" rel="self"/>
 <link href="http://bastost.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://tom.preston-werner.com/</id>
 <author>
   <name>Tiago Bastos</name>
   <email>_@bastost.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://bastost.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://bastost.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>